
@{
    ViewBag.Title = "LinkSend";
}

<h2>LinkSend</h2>

<div class="form-group">
    <div class="col-lg-12">
        <div class="card" style="min-height: 500px; padding: 10px;">
            <div class="card-header" style="background-color: #023877; color: #fff">
                <h4 class="card-title" id="basic-layout-colored-form-control">Generar Link</h4>
            </div>
            <div class="card-content  show">
                <div class="card-body">
                    <div class="Row">
                        <label>@ViewBag.Link</label>
                    </div>
                    <input type="button" class="btn-primary" value="Editar" onclick="window.location.href='@ViewBag.Link'" />

                </div>
            </div>

            
           
        </div>
    </div>
</div>

